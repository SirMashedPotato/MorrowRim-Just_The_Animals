    <label>bipedal</label>
    <corePart>
      <def>Body</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>Tail</def>
          <coverage>0.05</coverage>
        </li>
        <li>
          <def>Spine</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Stomach</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Heart</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Lung</def>
          <customLabel>left lung</customLabel>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Lung</def>
          <customLabel>right lung</customLabel>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>left kidney</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Kidney</def>
          <customLabel>right kidney</customLabel>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Liver</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>Neck</def>
          <coverage>0.3</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>Head</def>
              <coverage>0.75</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Skull</def>
                  <coverage>0.30</coverage>
                  <depth>Inside</depth>
                  <parts>
                    <li>
                      <def>Brain</def>
                      <coverage>0.60</coverage>
                      <depth>Inside</depth>
                    </li>
                  </parts>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.12</coverage>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.12</coverage>
                </li>
				<li>
                  <def>Ear</def>
                  <customLabel>left ear</customLabel>
                  <coverage>0.04</coverage>
                </li>
                <li>
                  <def>Ear</def>
                  <customLabel>right ear</customLabel>
                  <coverage>0.04</coverage>
                </li>
                <li>
                  <def>Nose</def>
                  <coverage>0.06</coverage>
                </li>
                <li>
                  <def>AnimalJaw</def>
                  <coverage>0.10</coverage>
                  <groups>
                    <li>Teeth</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left leg</customLabel>
          <coverage>0.075</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>left foot</customLabel>
              <coverage>0.5</coverage>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right leg</customLabel>
          <coverage>0.075</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>Foot</def>
              <customLabel>right foot</customLabel>
              <coverage>0.5</coverage>
              <groups>

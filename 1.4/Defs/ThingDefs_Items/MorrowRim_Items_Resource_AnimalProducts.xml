<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Netch jelly -->

	<ThingDef ParentName="OrganicProductBase">
		<defName>MorrowRim_NetchJelly</defName>
		<label>netch jelly</label>
		<description>A jelly like substance 'milked' from a netch. Can be eaten raw.\nHas unusual properties when combined with certain substances.</description>
		<graphicData>
		  <texPath>MorrowRim/Items/Resources/MorrowRim_NetchJelly</texPath>
		  <drawSize>0.8</drawSize>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
		  <MarketValue>3.1</MarketValue>
		  <Mass>0.045</Mass>
		  <MaxHitPoints>60</MaxHitPoints>
		  <Flammability>0.2</Flammability>
		  <Nutrition>0.05</Nutrition>
		  <FoodPoisonChanceFixedHuman>0.05</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
		  <li Class="CompProperties_Rottable">
			<daysToRotStart>8</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		  </li>
		</comps>
		<ingestible>
		  <preferability>RawTasty</preferability>
		  <foodType>AnimalProduct, Fluid</foodType>
		</ingestible>
		<thingCategories>
		  <li>AnimalProductRaw</li>
		</thingCategories>
		<tradeTags>
			<li>AshlandCommon</li>
		</tradeTags>
	</ThingDef>
	
	<!-- Nix ox 'milk' -->

	<ThingDef ParentName="OrganicProductBase">
		<defName>MorrowRim_NixOxMilk</defName>
		<label>nix ox mucus</label>
		<description>A viscous liquid secreted by a nix ox. Rather foul tasting when consumed raw, but not too bad when used in cooking.</description>
		<graphicData>
		  <texPath>MorrowRim/Items/Resources/MorrowRim_NixOxMilk</texPath>
		  <drawSize>0.8</drawSize>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
		  <MarketValue>1</MarketValue>
		  <Mass>0.045</Mass>
		  <MaxHitPoints>60</MaxHitPoints>
		  <Flammability>0.2</Flammability>
		  <Nutrition>0.05</Nutrition>
		  <FoodPoisonChanceFixedHuman>0.05</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
		  <li Class="CompProperties_Rottable">
			<daysToRotStart>14</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		  </li>
		</comps>
		<ingestible>
		  <preferability>RawBad</preferability>
		  <foodType>AnimalProduct, Fluid</foodType>
		  <specialThoughtDirect>MorrowRim_ateNixOxMilk</specialThoughtDirect>
		</ingestible>
		<thingCategories>
		  <li>AnimalProductRaw</li>
		</thingCategories>
		<tradeTags>
			<li>AshlandCommon</li>
		</tradeTags>
	</ThingDef>
	
	<!-- Shalk resin -->
	
	<ThingDef ParentName="ResourceBase">
		<defName>MorrowRim_ShalkResin</defName>
		<label>shalk resin</label>
		<description>Resin obtained by butchering a shalk. Often used in crafting bonemold and chitin armour, but can also be used when refining muck.</description>
		<graphicData>
		  <texPath>MorrowRim/Items/Resources/ShalkResin</texPath>
		  <graphicClass>Graphic_StackCount</graphicClass>
		  <drawSize>0.90</drawSize>
		</graphicData>
		<stackLimit>75</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<statBases>
		  <MaxHitPoints>50</MaxHitPoints>
		  <MarketValue>6</MarketValue>
		  <Mass>0.02</Mass>
		  <Flammability>0.5</Flammability>
		  <DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<thingCategories>
		  <li>Manufactured</li>
		</thingCategories>
		<tradeTags>
			<li>AshlandCommon</li>
		</tradeTags>
	</ThingDef>
	
</Defs>
